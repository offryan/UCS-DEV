algoritmo EX4
    real salM salP
    início
        escreva("Digite o valor do salário mínimo: R$ ")
        leia(salM)

        escreva("Digite o valor do seu salário: R$ ")
        leia(salP)

    fim
void calcular(real salP, real salM)
    total = salP/salM

    escreva("Você recebe" +total "salários mínimos")
    fim
    