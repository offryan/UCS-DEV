algoritmo EX11
    inteiro a, b, soma, sub, multi, div, rDiv
    in√≠cio
        escreva("Digite o valor de a: ")
        leia(a)
        escreva("Digite o valor de b: ")
        soma = a + b
        leia(soma)
        sub = a - b
        leia(sub)
        multi = a*b
        leia(multi)
        div = a/b
        leia(div)
        rDiv = Math.floor(a/b)
        leia(rDiv)
    fim