algoritmo EX2
    real area, lado1, lado2
    início
        calculaArea()
        escreva("Digite o primeiro lado: ")
        leia(lado1)
        escreva("Digite o segundo lado: ")
        leia(lado2)
    fim
void calculaArea(real area, real lado1, real lado2)
    início
        area = lado1*lado2
        escreva("O valor da área é: " + area +)
    fim